<% var item, key %>

<% htmlWebpackPlugin.options.appMountIds = htmlWebpackPlugin.options.appMountIds || [] %>
<% htmlWebpackPlugin.options.lang = htmlWebpackPlugin.options.lang || "en" %>
<% htmlWebpackPlugin.options.links = htmlWebpackPlugin.options.links || [] %>
<% htmlWebpackPlugin.options.meta = htmlWebpackPlugin.options.meta || [] %>
<% htmlWebpackPlugin.options.scripts = htmlWebpackPlugin.options.scripts || [] %>

<!DOCTYPE html>
<html lang="<%= htmlWebpackPlugin.options.lang %>" <% if (htmlWebpackPlugin.files.manifest) { %> manifest="<%= htmlWebpackPlugin.files.manifest %>"<% } %>>
  <head>
    <meta charset="utf-8">
    <meta content="ie=edge" http-equiv="x-ua-compatible">

    <% if (htmlWebpackPlugin.options.baseHref) { %>
    <base href="<%= htmlWebpackPlugin.options.baseHref %>">
    <% } %>

    <% if (Array.isArray(htmlWebpackPlugin.options.meta)) { %>
      <% for (item of htmlWebpackPlugin.options.meta) { %>
      <meta<% for (key in item) { %> <%= key %>="<%= item[key] %>"<% } %>>
      <% } %>
    <% } %>

    <title><%= htmlWebpackPlugin.options.title %></title>

    <% if (htmlWebpackPlugin.files.favicon) { %>
    <link href="<%= htmlWebpackPlugin.files.favicon %>" rel="shortcut icon">
    <% } %>

    <% if (htmlWebpackPlugin.options.mobile) { %>
    <meta content="width=device-width, initial-scale=1" name="viewport">
    <% } %>

    <% for (item of htmlWebpackPlugin.options.links) { %>
    <% if (typeof item === 'string' || item instanceof String) { item = { href: item, rel: 'stylesheet' } } %>
    <link<% for (key in item) { %> <%= key %>="<%= item[key] %>"<% } %>>
    <% } %>

    <% for (key in htmlWebpackPlugin.files.css) { %>
    <link href="<%= htmlWebpackPlugin.files.css[key] %>" rel="stylesheet">
    <% } %>
    <style type="text/css">
:root {
  --bg-color: #fff;
  --color: #000;
  --invert-bg-color: #333;
  --invert-color: #bbb;
  --link-color: #11bfa5;
  --header-height: 3rem;
  --card-header-bg: #ddd;
  --color-alt: #666;
  --layout-max-width: 850px;
  /* ------------ */
  font-size: 13px;
  line-height: 1.618;
}
@media screen and (min-width: 600px) {
  :root {
    font-size: calc(13px  +  6  * ((100vw -  600px ) / 300 ));
  }
}
@media screen and (min-width: 900px) {
  :root {
    font-size: 20px;
  }
}


*,
*:before,
*:after {
  box-sizing: border-box;
}

body {
  font-family: monospace;
  margin: 0;
  padding: 10vh 0 0 0;
  overflow: auto;
}

a,
a:link,
a:visited {
  color: var(--link-color, #11bfa5);
  text-decoration: none;
  text-shadow: -2px 0 1px var(--bg-color, #fff),
               2px 0 1px var(--bg-color, #fff),
               -2px 0 0 var(--bg-color, #fff),
               2px 0 0 var(--bg-color, #fff),
               -1px 0 1px var(--bg-color, #fff),
               1px 0 1px var(--bg-color, #fff),
               -1px 0 0 var(--bg-color, #fff),
               1px 0 0 var(--bg-color, #fff);
  box-shadow: 0 0 0 0 var(--bg-color, #fff) inset,
              0 0 0 0 var(--bg-color, #fff) inset;
  transition: box-shadow 0.218s ease;
}
a:hover {
  box-shadow: 0 -2px 0 0 var(--bg-color, #fff) inset,
              0 -3px 0 0 currentColor inset;
}

h1, h2, h3 {
  font-weight: 100;
}
body > header {
  width: 100%;
}

body {
  background-image: url(./static/header-bg-l.jpg);
  background-size: cover;
  background-attachment: fixed;
  background-position: 50% 50%;
}
/*
@media screen and (min-width: 600px) {
  body {
    background-image: url(./static/header-bg-l.jpg);
  }
}
*/
@media screen and (min-width: 900px) {
  body {
    background-image: url(./static/header-bg-xl.jpg);
  }
}

body > header {
  display: none;
}

body > header > img {
  display: block;
  margin: auto;
  max-width: 100%;
  max-height: 50vh;
}

body > section {
  background-color: var(--bg-color, #fff);
}

body > section {
  margin-top: 25vh;
  overflow: auto;
}
body > section:first-of-type {
  margin-top: 0;
}
body > section:before {
  display: block;
  content: '';
}

body > section > header,
body > section > main,
body > section > footer,
body > footer > nav {
  padding: 0 1em;
  margin-left: auto;
  margin-right: auto;
  max-width: var(--layout-max-width, 850px);
}

body > section:before:first-of-type {
  display: none;
}

body > footer {
  font-size: 0.8rem;
  background-color: var(--bg-color, #fff);
  margin-top: 10vh;
}

body > footer ul {
  text-align: center;
  display: block;
  list-style: none;
  margin: 0;
  padding: 0;
}
body > footer li {
  display: inline-block;
}


dt:after {
  margin-top: 2em;
  content: ':';
  display: inline;
}
dd {
  margin-bottom: 2em;
}

dd > ul {
  margin-top: 0;
  padding-left: 0;
  list-style-position: outside;
}


.note {
  color: var(--color-alt, #666);
  font-size: 0.75em;
}





.x-rows,
.x-columns,
.x-r-rows,
.x-r-columns {
  display: flex;
}
.x-columns {
  flex-direction: row;
}
.x-r-columns {
  flex-direction: row-reverse;
}
.x-r-rows {
  flex-direction: column-reverse;
}
.x-shrink {
  flex-shrink: 2;
}
.x-shrink-l {
  flex-shrink: 3;
}
.x-shrink-xl {
  flex-shrink: 4;
}
.x-grow {
  flex-grow: 2;
}
.x-grow-l {
  flex-grow: 3;
}
.x-grow-xl {
  flex-grow: 4;
}


#hello main {
  display: flex;
  flex-wrap: wrap;
}
#hello main > div {
  width: 100%;
}
@media screen and (min-width: 600px) {
  #hello main > div {
    font-size: 0.85em;
    width: calc(50% - 1rem);
    margin: 1rem;
  }
  #hello main > div:nth-child(2n+1) {
    margin-left: 0;
  }
  #hello main > div:nth-child(2n+0) {
    margin-right: 0;
  }
}

